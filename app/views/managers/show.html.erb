<div class="pull-right">
  <%= link_to "Back", managers_path, class: "btn btn-success" %>
</div>
<div class="container">
  <div class="container table-container">
    <div class="col-md-12">
      <h1>Information of Manager</h1>
    </div>   
    <br>
    <table class="table table-bordered">
      <thead>
        <tr>
          <th>#</th>
          <th>Hospital Id</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><%= @user.id %></td>
          <td><%= @user.hospital_id %></td>
          <td><%= @user.first_name %></td>
          <td><%= @user.last_name %></td>
          <td><%= @user.email %></td>
        </tr>
      </tbody>
    </table>
  </div>
</div>


