
<div class="container">
  <div class="row">
    <div class="col-md-2  col-md-offset-12 ">
      <%= link_to "Back", managers_path, class: "btn btn-success" %>
    </div>
  </div>
 <div class="container table-container">
     <h1>Information of Manager</h1>
    <br>
  <div class="row">
    <div class="col-md-12">
      <table class="table table-bordered">
          <thead>
            <tr>
              <th>#</th>
              <th>Hospital Id</th>
              <th>First Name</th>
              <th>Last Name</th>
              <th>Email</th>
           </tr>
          </thead>

        <tbody>
            <tr>
              <td><%= @user.id %></td>
              <td><%= @user.hospital_id %></td>
              <td><%= @user.first_name %></td>
              <td><%= @user.last_name %></td>
              <td><%= @user.email %></td>
            </tr>
        </tbody>
      </table>
    </div>
  </div>
 </div>
</div>

