<div class="container i-box">
 <div class="row">
  <div class="col-md-2  col-md-offset-12 ">
    <%= link_to "Back",  hospital_user_babies_path, class: "btn btn-success" %>
  </div>
  <div class="col-md-10 col-md-offset-1">
    <h3>Show Information of One Baby</h3>
  </div>
    <div class="row">
    <div class="col-md-8  col-md-offset-1">
      <table class="table">
        <thead>  
          <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Date Of Birth</th>
            <th>Place Of Birth</th>
            <th>Gender</th>
            <th>Age</th>
            <th>Father Id</th>
            <th>Mother Id</th>
          </tr>
        </thead> 
        <tbody>
            <tr>
              <td><%= @baby.id %></td> 
              <td><%= @baby.first_name %></td> 
              <td><%= @baby.date_of_birth %></td>
              <td><%= @baby.place_of_birth %></td> 
              <td><%= @baby.sex %></td>
              <td><%= @baby.age %></td>
              <td><%= @baby.father_id %></td>
              <td><%= @baby.mother_id %></td>
       </table>      
     </div>    
   </div>
 </div>
</div>











  